        android:id="@+id/crop_info_Fragment"
        android:name="com.android.farmist.fragments.Crop_info_Fragment"
        android:label="fragment_crop_info_"
        tools:layout="@layout/fragment_crop_info_"

        />
    <fragment
        android:id="@+id/selectCropCategory"

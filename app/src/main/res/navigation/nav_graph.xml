            android:id="@+id/action_nav_home_to_myFarm_Fragment"
            app:destination="@id/myFarm_Fragment" />
        <action
            android:id="@+id/action_nav_home_to_upcomingAction"
            app:destination="@id/upcomingAction" />
    </fragment>
    <fragment

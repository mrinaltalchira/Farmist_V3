    <fragment
        android:id="@+id/loan_Subsidies_Fragment"
        android:name="com.android.farmist.fragments.Loan_Subsidies_Fragment"
        android:label="Loan_Subsidies_Fragment"
        tools:layout="@layout/loans_subsidies_info"/>
